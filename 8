import turtle
turtle.speed(0)
turtle.fillcolor("aqua")
turtle.begin_fill()
turtle.left(90)
turtle.forward(200)
turtle.left(120)
turtle.forward(200)
turtle.left(120)
turtle.forward(200)
turtle.end_fill()
turtle.penup()
turtle.goto(-130,100)
turtle.dot()
turtle.goto(86,50)

turtle.setheading(90)
turtle.pendown()
turtle.begin_fill()
turtle.forward(100)
turtle.left(120)
turtle.forward(100)
turtle.left(120)
turtle.forward(100)
turtle.end_fill()
